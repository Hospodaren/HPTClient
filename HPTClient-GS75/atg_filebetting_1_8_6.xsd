			<xs:element name="v85Coupon" type="v85CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="v75Coupon" type="v75CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="v65Coupon" type="v65CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="v64Coupon" type="v64CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="v5Coupon" type="v5CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="v4Coupon" type="v4CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="v3Coupon" type="v3CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="gs75Coupon" type="gs75CouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="DDCoupon" type="DDCouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="LDCoupon" type="LDCouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="kombCoupon" type="kombCouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="tvillingCoupon" type="tvillingCouponType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="trioCoupon" type="trioCouponType" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="issuer">
		<xs:annotation>
			<xs:documentation>Root element. Contains information about file generation.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="betcoupons" type="betcouponsType"/>
			</xs:sequence>
			<xs:attribute name="company" use="optional">
				<xs:annotation>
					<xs:documentation>Name of the company issuing the product that created the file.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="product" use="optional">
				<xs:annotation>
					<xs:documentation>Name of the product that created the file.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="version" use="optional">
				<xs:annotation>
					<xs:documentation>Version of the product that created the file.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="64"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="createddate" type="xs:date" use="optional">
				<xs:annotation>
					<xs:documentation>The date that the file was created (yyyy-mm-dd).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="createdtime" type="xs:time" use="optional">
				<xs:annotation>
					<xs:documentation>The time of day that the file was created (hh:mm:ss). 24-hr format.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="schemaversion" type="xs:string" fixed="ATG File Betting XSD ver 1.8">
				<xs:annotation>
					<xs:documentation>Version of the XML Schema Definition (XSD) with which the XML-file was validated. This is a fixed string.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="legType">
		<xs:annotation>
			<xs:documentation>A leg in a multi-leg bet type.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="legno" use="required">
			<xs:annotation>
				<xs:documentation>The number (order) of the leg in a multi-leg bet. Must be a value between 1 and 8.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="8"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="marks" use="required">
			<xs:annotation>
				<xs:documentation>A sequence of fifteen "0" or "1", representing a mark (1) or no mark (0), for the runner in starting position 1 through 15, from left to right. E.g. the string "010100000010000" would be interpreted as a leg containing three marks, for the runners designated the starting positions 2, 4 and 11.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[0-1]{15}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="r1" use="optional">
			<xs:annotation>
				<xs:documentation>The optional choice of first reserve. Must be a number between 1 and 15. </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="15"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="r2" use="optional">
			<xs:annotation>
				<xs:documentation>The optional choice of second reserve. Must be a number between 1 and 15. </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="15"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="legType20">
		<xs:annotation>
			<xs:documentation>A leg in a multi-leg bet type of 20 selections.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="legno" use="required">
			<xs:annotation>
				<xs:documentation>The number (order) of the leg in a multi-leg bet. Must be a value between 1 and 8.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="8"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="marks" use="required">
			<xs:annotation>
				<xs:documentation>A sequence of twenty "0" or "1", representing a mark (1) or no mark (0), for the runner in starting position 1 through 20, from left to right. E.g. the string "01010000001000010000" would be interpreted as a leg containing three marks, for the runners designated the starting positions 2, 4, and 16.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[0-1]{20}"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="r1" use="optional">
			<xs:annotation>
				<xs:documentation>The optional choice of first reserve. Must be a number between 1 and 20. </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="20"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="r2" use="optional">
			<xs:annotation>
				<xs:documentation>The optional choice of second reserve. Must be a number between 1 and 20. </xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="20"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="v4CouponType">
		<xs:annotation>
			<xs:documentation>The coupon for the V4 (Pick4) bet type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="leg" type="legType20" minOccurs="4" maxOccurs="4"/>
		</xs:sequence>
		<xs:attribute name="couponid" use="required">
			<xs:annotation>
				<xs:documentation>The sequence number of the coupon in the file, starting with 1 for the first coupon and incrementing by 1 for each additional coupon contained in the file.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="9999"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="date" type="xs:date" use="required">
			<xs:annotation>
				<xs:documentation>The date of the meeting that the bet concerns. (yyyy-mm-dd)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="trackcode" use="required">
			<xs:annotation>
				<xs:documentation>The numeric code for the track where the meeting is held that the bet concerns. Min 1, max 99.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="99"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="betmultiplier" use="optional" default="1">
			<xs:annotation>
				<xs:documentation>The number of times that the bet (coupon) will be entered. Default and minimum is 1, maximum is 100. The following multipliers are valid: 1 (default), 2, 3, 4, 5, 10, 50, 100.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:integer">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="100"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
